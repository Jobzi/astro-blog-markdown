{
  "name": "astro-blog-markdown",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && subfont -ir --root dist",
    "preview": "astro preview",
    "check": "astro check --fail-on-warnings",
    "check:svelte": "svelte-check --fail-on-hints",
    "format": "prettier --write --plugin-search-dir=. .",
    "lint:css": "stylelint \"src/**/*.{astro,css,svelte}\"",
    "prettier:check": "prettier --check --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@astrojs/svelte": "^0.0.2",
    "@commitlint/config-conventional": "^16.2.1",
    "@iconify/svelte": "^2.2.1",
    "@netlify/plugin-lighthouse": "^2.1.3",
    "astro": "^0.25.4",
    "autoprefixer": "^10.4.4",
    "commitlint": "^16.2.3",
    "husky": "^7.0.4",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "prettier": "^2.6.1",
    "prettier-plugin-astro": "^0.0.12",
    "stylelint": "^14.6.1",
    "stylelint-config-recommended": "^7.0.0",
    "svelte": "^3.46.6",
    "svelte-check": "^2.4.6",
    "vite": "^2.9.1"
  },
  "dependencies": {
    "subfont": "^6.4.1"
  }
}
